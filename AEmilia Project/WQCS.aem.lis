(492)
(493)		FROM QSS.send_request 														TO WCD.receive_qss_request;
(494)		FROM QSQC.send_request 														TO WCD.receive_qsqc_request;
(495)		FROM WCU.forward_qss 														TO QSS.receive_response;
(496)		FROM WCU.forward_qsqc														TO QSQC.receive_response;
(497)		FROM WCD.forward_qss 														TO CCS.receive_wired_StartUpSampling_request;
(498)		FROM WCD.forward_qsqc														TO CCS.receive_wired_CheckWater_request;
(499)		FROM CCS.send_wired_StartUpSampling_response  								TO WCU.receive_qss_response;
(500)		FROM CCS.send_wired_CheckWater_response 									TO WCU.receive_qsqc_response;
(501)		FROM CCS.send_wired_getExpectedParametersInland_request						TO ICD.receive_pss_params_request;
(502)		FROM CCS.send_wired_getExpectedParametersOutGoing_request 					TO ICD.receive_WCS_params_request;
(503)		FROM CCS.send_sample 														TO ICD.receive_save_sample_request;
(504)		FROM CCS.send_StartUpSampling_request 										TO INCU.receive_StartUpSampling_request; % Controllare se si fa cosÃ¬
(505)		FROM CCS.send_StartUpSampling_request 										TO OUTCU.receive_StartUpSampling_request;
(506)		FROM INCU.send_StartUpSampling_response 									TO CCS.receive_StartUpSampling_response;
(507)		FROM INCU.send_sample 														TO CCS.receive_sample;
(508)		FROM INCU.send_sample_request 												TO WLCD.receive_incu_request;
(509)		FROM OUTCU.send_StartUpSampling_response 									TO CCS.receive_StartUpSampling_response;
(510)		FROM OUTCU.send_sample 														TO CCS.receive_sample;
(511)		FROM OUTCU.send_sample_request 												TO WLCD.receive_outcu_request;
(512)		FROM WLCU.forward_incu 														TO INCU.receive_sample_response;
(513)		FROM WLCU.forward_outcu 													TO OUTCU.receive_sample_response;
(514)		FROM ICD.forward_pss_params 												TO PSS.getExpectedParametersInland_request;
(515)		FROM ICD.forward_save_sample 												TO WCS.save_sample_request;
(516)		FROM ICD.forward_WCS_params 												TO WCS.getExpectedParametersOutGoing_request;
(517)		FROM ICU.forward_pss_params 												TO CCS.receive_wired_getExpectedParametersInland_response;
(518)		FROM ICU.forward_WCS_params 												TO CCS.receive_wired_getExpectedParametersOutGoing_response;
