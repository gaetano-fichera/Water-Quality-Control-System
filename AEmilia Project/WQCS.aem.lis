(460)		ARCHI_ELEM_INSTANCES
(461)			WCS : WaterCompanyServer_Type(SampleWaterCompanyServer_rate, CheckWaterCompanyServer_rate);
(462)			WCU: WiredConnectionUp_Type(WiredConnectionUp_rate);
(463)			WCD : WiredConnectionDown_Type(WiredConnectionDown_rate);
(464)			WCDISK : WaterCompanyDisk_Type(SampleWaterCompanyDisk_rate, CheckWaterCompanyDisk_rate);
(465)			QSS : QSSamplingApp_Type(QSSamplingApp_request_rate);
(466)			QSQC : QSQualityControlApp_Type(QSQualityControlApp_request_rate);
(467)			CCS : ControlCenterServer_Type(CCS_send_StartUpSampling_request_rate, CCS_CheckWaterInland_prob_request, CCS_send_wired_getExpectedParametersInland_request_rate,
(468)				CCS_send_wired_getExpectedParametersOutGoing_request_rate, CCS_send_wired_StartUpSampling_response_rate, CCS_send_sample_rate, CCS_send_wired_CheckWaterInland_response_rate,
(469)				CCS_send_wired_CheckWaterOutGoing_response_rate);
(470)			INCU : SeaweedPickingControlUnit_Type(SPCU_send_StartUpSampling_response_rate, SPCU_send_sample_request_rate, SPCU_send_sample_rate, SPCU_delay_rate);
(471)			OUTCU : SeaweedPickingControlUnit_Type(SPCU_send_StartUpSampling_response_rate, SPCU_send_sample_request_rate, SPCU_send_sample_rate, SPCU_delay_rate);
(472)			WLCU : WirelessConnectionUp_Type(WCU_forward_outcu_rate, WCU_forward_incu_request_rate);
(473)			WLCD : WirelessConnectionDown_Type(WCD_forward_outcu_rate, WCD_forward_incu_request_rate);
(474)			ICD : InternetConnectionDown_Type(ICD_forward_save_sample_rate, ICD_forward_pss_params_rate, ICD_forward_WCS_params_rate);
(475)			ICU : InternetConnectionUP_Type(ICU_forward_pss_params_rate, ICU_forward_WCS_params_rate);
(476)			PSS : PurificationSystemServer_Type(PSS_getExpectedParametersInland_response_rate);
(477)
(478)			FOR_ALL i IN 0..sensorsIN - 1
(479)				SPIN[i] : SeaweedPicking_Type(SP_send_sample_response_rate);
(480)
(481)			FOR_ALL i IN 0..sensorsOUT - 1
(482)				SPOUT[i] : SeaweedPicking_Type(SP_send_sample_response_rate)
